1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"card col-12 col-md-6\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img\">\n                  <span mbr-icon class=\"mbr-iconfont icon-1 mbri-touch-swipe\"></span>\n                </div>\n                <div class=\"card-box\">\n                  <h5 class=\"link align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showLink\" data-app-selector=\".link\"><a href=\"https://docs.google.com/spreadsheets/d/15Q56Hui9414Ab_i-BIeY_B5ZYtBdiplae72sj6emC0Q/edit#gid=0\" class=\"text-link text-black\">USO CATÁLOGO</a></h5>\n                </div>\n                <div class=\"card-icon\">\n                  <span mbr-icon class=\"mbr-iconfont icon-arrow mobi-mbri-right mobi-mbri\"></span>\n                </div>\n              </div>\n            </div>\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 1\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img\">\n                  <span mbr-icon class=\"mbr-iconfont mobi-mbri-desktop mobi-mbri icon-1\"></span>\n                </div>\n                <div class=\"card-box\">\n                  <h5 class=\"link align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showLink\" data-app-selector=\".link\"><a href=\"https://docs.google.com/document/d/1ENx9K52-9col0q5rtJHxh8BP7jU3MDyCX7staefv1sg/edit\" class=\"text-link text-bl